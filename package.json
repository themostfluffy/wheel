{
  "name": "go-live-button",
  "displayName": "Go Live Button",
  "description": "A button on the status bar to start a local server",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "goLive.startServer",
        "title": "Go Live: Start Local Server"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0"
  }
}
